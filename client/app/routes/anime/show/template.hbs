<h1>Anime Page</h1>

<h2 data-test-selector="title">
  {{anime.canonicalTitle}}
</h2>

{{#if currentSession.isAuthenticated}}
  {{library-status-button
      entry=entry
      promise=entryPromise
      mediaType="anime"
      create=(route-action "createEntry")
      update=(route-action "updateEntry" entry)
      delete=(route-action "destroyEntry" entry)
  }}
{{/if}}
